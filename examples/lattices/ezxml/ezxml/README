cd $UAL/examples/lattices/ezxml/

tar xvfz ezxml-0.8.6.tar.gz

cd ezxml

make

gcc element.c ./ezxml.o
      ./a.out SBMihsl
      HIT
      SBMihsl:
      type sbend:
      l 1.0799228:
      a 0:
      b 0.09816498252000001:

gcc sectors.c ./ezxml.o
      ./a.out ring
      HIT
      mbegin:
      RF1:
      octantDF:
      octantFD:
      mQuarter:
      octantDF:
      octantFD:
      mHalf:
      octantDF:
      octantFD:
      mQuarter:
      octantDF:
      octantFD:
      mend:

gcc elements.c ./ezxml.o
      ./a.out
