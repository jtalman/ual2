echo "_______________________________________________________________________"
echo "_______________________________________________________________________"
ls -l /usr/lib/libstdc++.so.6*
echo "_______________________________________________________________________"

ls -l /lib/libm.*
ls -l /lib/libm-*
echo "_______________________________________________________________________"

ls -l /lib/libgcc_s.*
ls -l /lib/libgcc_s-*
echo "_______________________________________________________________________"

ls -l /lib/libc.*
ls -l /lib/libc-*
echo "_______________________________________________________________________"

ls -l /usr/bin/gcc*
echo "_______________________________________________________________________"

ls -l /usr/bin/g++*
echo "_______________________________________________________________________"
echo "_______________________________________________________________________"

echo "2018 Libraries"
ls -l /usr/lib/libstdc++.so.6* | grep 2018
ls -l /lib/libm.* | grep 2018
ls -l /lib/libm-* | grep 2018
ls -l /lib/libgcc_s.* | grep 2018
ls -l /lib/libgcc_s-* | grep 2018
ls -l /lib/libc.* | grep 2018
ls -l /lib/libc-*  | grep 2018
ls -l /usr/bin/gcc* | grep 2018
ls -l /usr/bin/g++* | grep 2018
echo "_______________________________________________________________________"
echo "_______________________________________________________________________"

echo "2021 Libraries"
ls -l /usr/lib/libstdc++.so.6* | grep 2021
ls -l /lib/libm.* | grep 2021
ls -l /lib/libm-* | grep 2021
ls -l /lib/libgcc_s.* | grep 2021
ls -l /lib/libgcc_s-* | grep 2021
ls -l /lib/libc.* | grep 2021
ls -l /lib/libc-*  | grep 2021
ls -l /usr/bin/gcc* | grep 2021
ls -l /usr/bin/g++* | grep 2021
echo "_______________________________________________________________________"
echo "_______________________________________________________________________"
